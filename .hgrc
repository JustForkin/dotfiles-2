[ui]
username = Naoya INADA <naoina@naniyueni.org>
merge = /usr/bin/vimdiff
ignore = .hg/hgignore

[web]
allow_archive = bz2 gz zip
encoding = UTF-8
cacerts = /etc/ssl/certs/ca-certificates.crt

[merge-tools]
checkconflicts = True

[extensions]
convert =
rebase =
record =
color =
mq =
eol =
graphlog =
bookmarks =
transplant =
hggit =
svn = ~/hg-extension/hgsubversion/hgsubversion
histedit = ~/hg-extension/histedit/hg_histedit.py
hgshelve = ~/hg-extension/hgshelve/hgshelve.py

[alias]
show = diff -r -2:tip
log = glog

[hgsubversion]
externals = subrepos

[bookmarks]
track.current = True
