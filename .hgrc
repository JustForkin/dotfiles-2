[ui]
username = Naoya Inada <naoina@kuune.org>
# ignore = .hg/hgignore

[web]
allow_archive = bz2 gz zip
encoding = UTF-8
cacerts = /etc/ssl/certs/ca-certificates.crt

[merge-tools]
checkconflicts = True

[extensions]
convert =
rebase =
record =
color =
mq =
eol =
graphlog =
bookmarks =
transplant =
purge =

[alias]
show = diff -c tip
log = glog
resolve = resolve --tool /usr/bin/vimdiff

[hgsubversion]
externals = subrepos

[bookmarks]
track.current = True
